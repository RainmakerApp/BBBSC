    .body__hero(style='background-image: url({event.photo_url})')
      .content
        h1 {event.name}
        //@IF event.prop.presented-by
        small Presented By: 
          strong {event.prop.presented-by}
        //@ENDIF
        p <span style="text-transform: uppercase;">{event.prop.start-time} {event.prop.start-time-meridiem} MT</span> | {event.timestamp.start.unix|date(l)}, {event.timestamp.start.unix|date(M)} {event.timestamp.start.unix|date(jS Y)}
        p {event.location_name}
      .hero__overlay
      .button-group: ul
        li.share Invite Friends
          ul.options
            li: a(href='https://www.facebook.com/dialog/feed?app_id=929983597012726&display=popup&href=http://bbbsc.donate.io/events/%23/event/{event.id}&redirect_uri=http://bbbsc.donate.io/events/%23/event/{event.id}&description={event.summary|strip_tags}&caption={event.name}&picture={:e.url.link([event.photo_url])}', class='popup') Facebook
            li: a(href='http://twitter.com/home?status={event.summary|strip_tags|limitstring(80,...)} - www.bbbsc.donate.io/events/#/event/{event.id}', class='twitter-share popup') Twitter
            li: a(href='mailto:?subject={event.name} - Big Brothers Big Sisters of Colorado&body=www.bbbsc.donate.io/events/#/event/{event.id}') Email
        li.middle: span(class='addtocalendar atc-style-menu-wb', data-on-button-click='atcButtonClick', data-on-calendar-click='atcCalendarClick')
          .atcb-link Add To Calendar
          var.atc_event
            //@IF event.prop.start-time
            var.atc_date_start {event.timestamp.start.unix|date(o-m-d)} {event.prop.start-time}:00
            //@ENDIF
            //@IF event.prop.end-time
            var.atc_date_end {event.timestamp.until.unix|date(o-m-d)} {event.prop.end-time}:00
            //@ENDIF
            var.atc_timezone America/Chicago
            var.atc_title {event.name}
            var.atc_description {event.summary|strip_tags}
            var.atc_location {event.location}
            var.atc_organizer {event.owner.name}
            //- {event.owner|dump}
            //- var.atc_organizer_email luke@starwars.com
        li.disabled Save This Event
