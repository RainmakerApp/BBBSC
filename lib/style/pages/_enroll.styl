.page.page-enroll
	color $text
	.jumbotron
		background-image url(../images/banners/enroll.png)
		position relative
		.language
			background #FFF
			bottom 0
			color $green
			cursor pointer
			left 5%
			padding 10px 20px
			position absolute
	.container#main
		padding-bottom 60px
		padding-top 60px
	.table.programs
		h2
			@extend .tr-up
		strong
			display block
			margin-bottom 5px
		ul li
			margin-bottom 30px
			a
				border 0
				text-decoration underline
	.section
		background #f5f5f5
		color #575757
		font-size 20px
		font-weight 100
		line-height 32px
		padding-bottom 30px
		padding-top 30px
		clearfix()
		p.sub
			margin-bottom 60px